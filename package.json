{
  "name": "toss-ui",
  "version": "0.1.0",
  "description": "Toss Design System (TDS) components built with React, Tailwind CSS, and shadcn/ui",
  "type": "module",
  "author": "Brendan Hwang",
  "license": "MIT",
  "main": "./dist/toss-ui.cjs",
  "module": "./dist/toss-ui.js",
  "types": "./dist/lib.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/toss-ui.js",
      "require": "./dist/toss-ui.cjs"
    },
    "./styles.css": "./dist/toss-ui.css",
    "./dist/toss-ui.css": "./dist/toss-ui.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "keywords": [
    "toss",
    "design-system",
    "react",
    "tailwindcss",
    "shadcn",
    "ui-components"
  ],
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build && tsc --declaration --emitDeclarationOnly --outDir dist",
    "build:lib": "vite build && tsc --declaration --emitDeclarationOnly --outDir dist",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepublishOnly": "npm run build:lib"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.564.0",
    "radix-ui": "^1.4.3",
    "tailwind-merge": "^3.4.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "next-themes": "^0.4.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.13.0",
    "shadcn": "^3.8.5",
    "sonner": "^2.0.7",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.3.1"
  }
}
